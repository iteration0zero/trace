; Part 2: omega curriculum
; Using literals instead of val_n
(learn-from (
  (n (omega n))
  ((n n) (omega (n n)))
  ((n (n n) (n n n)) (omega (n (n n) (n n n))))
  (k (omega k))
  (i (omega i))
  (s (omega s))
  (Z (omega Z))
  ((Z Z Z Z) (omega (Z Z Z Z)))
  (((k k k) s s) (omega (((k k k) s s))))
  (((k s k) s k) (omega (((k s k) s k))))
  (((s k s) k s) (omega (((s k s) k s))))
  ((s s s s s s s s s s s s) (omega (s s s s s s s s s s s s)))
  (((n n n) ((n n) (n n n))) (omega ((n n n) ((n n) (n n n)))))
  (wait2 (omega wait2))
  (swap (omega swap))
  (rest (omega rest))
) 5000)

; Part 3: wait1
(learn-from (
  (k (fn y (fn z (k y z))))
  (i (fn y (fn z (i y z))))
  ((k i) (fn y (fn z ((k i) y z))))
) 2000)

; Part 4: wait2
(learn-from (
  (k (fn y (fn z (fn w (k y z w)))))
  (i (fn y (fn z (fn w (i y z w)))))
  ((k i) (fn y (fn z (fn w ((k i) y z w)))))
) 2500)

; Part 5: swap
; Use (swap input) to generate expected output
(learn-from (
  (n (swap n))
  ((n n) (swap (n n)))
  ((n n n) (swap (n n n)))
  (k (swap k))
  (i (swap i))
  ((n (n n) n) (swap (n (n n) n)))
) 2000)

